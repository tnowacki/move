//# publish --address 0x1
module Tester {
    t() {
        let x: u64;
        let y: u64;
        let r1: &mut u64;

        x = 0;
        y = 0;
        r1 = Self.foo(&mut x, &mut y);
        // can copy even though data is "owned" by r1. bytecode verifier does not check ref trans
        _ = copy(x);
        return;
    }

    foo(r: &mut u64, r2: &mut u64): &mut u64 {
        return move(r2);
    }
}
