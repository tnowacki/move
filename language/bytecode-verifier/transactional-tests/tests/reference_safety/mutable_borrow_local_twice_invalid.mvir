//# publish
module 0x1.M {
    t1() {
        let a: u64;
        let r1: &mut u64;
        let r2: &mut u64;
        a = 0;
        r1 = &mut a;
        r2 = &mut a;
        // can now right to both! neither is an extension of the other
        *move(r2) = 2;
        *move(r1) = 1;
        return;
    }
}
