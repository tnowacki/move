module Tester {
    struct T has copy { f: u64 }

    t() {
        let x: Self.T;
        let r1: &mut u64;
        let f: u64;

        x = T { f: 0 };
        r1 = &mut (&mut x).f;
        // copy used to be invalid due to mut borrow
        T { f: f } = copy(x);
        _ = move(r1);
        T { f: f } = move(x);
        return;
    }
}

// check: Success
