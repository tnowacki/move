module M {
    t0(cond: bool) {
        let x: u64;
        let x_ref: &mut u64;
        x = 0;
        x_ref = &mut x;
        if (move(cond)) {
            *move(x_ref) = 1;
        }
        // Safe because x_ref can no longer be written to
        x = 1;
        return;
    }

    t1(cond: bool) {
